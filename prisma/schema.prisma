generator client {
    provider             = "prisma-client-py"
    interface            = "sync" // or "asyncio"
    recursive_type_depth = 5
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Skill {
    id          Int    @id @default(autoincrement())
    skillName   String
    description String
    category    String
    level       String // (e.g., "Beginner", "Advanced")
}